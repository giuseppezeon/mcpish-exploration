{
  "name": "operate_machine_interface",
  "version": "1.0.0",
  "description": "Operate machine interface (open/close doors, lids, etc.)",
  "tier": "T2",
  "input_schema": {
    "type": "object",
    "properties": {
      "machine_id": {"type": "string"},
      "operation": {
        "type": "string",
        "enum": ["open_door", "close_door", "open_lid", "close_lid"]
      }
    },
    "required": ["machine_id", "operation"]
  },
  "output_schema": {
    "type": "object",
    "properties": {
      "success": {"type": "boolean"},
      "operation_completed": {"type": "string"}
    }
  },
  "preconditions": ["machine_ready"],
  "postconditions": ["machine_interface_operated"],
  "timeout_s": 30,
  "safety": {
    "machine_interlocks": true,
    "force_limits": true
  },
  "failure_modes": [
    {
      "mode": "machine_not_ready",
      "mitigation": "wait_and_retry"
    },
    {
      "mode": "operation_failed",
      "mitigation": "retry_operation"
    }
  ],
  "retry_policy": {
    "max_attempts": 2,
    "backoff_factor": 1.5
  },
  "cost_model": {
    "estimated_duration": 15,
    "energy_cost": "low"
  }
}
