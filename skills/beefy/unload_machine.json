{
  "name": "unload_machine",
  "version": "1.0.0",
  "description": "Unload samples from a laboratory machine",
  "tier": "T2",
  "input_schema": {
    "type": "object",
    "properties": {
      "machine_id": {"type": "string"},
      "sample_id": {"type": "string"}
    },
    "required": ["machine_id", "sample_id"]
  },
  "output_schema": {
    "type": "object",
    "properties": {
      "success": {"type": "boolean"},
      "sample_retrieved": {"type": "boolean"}
    }
  },
  "preconditions": ["machine_ready", "sample_accessible"],
  "postconditions": ["sample_unloaded"],
  "timeout_s": 60,
  "safety": {
    "machine_interlocks": true,
    "sample_protection": true
  },
  "failure_modes": [
    {
      "mode": "sample_stuck",
      "mitigation": "adjust_grasp_and_retry"
    },
    {
      "mode": "machine_not_ready",
      "mitigation": "wait_for_completion"
    }
  ],
  "retry_policy": {
    "max_attempts": 2,
    "backoff_factor": 1.5
  },
  "cost_model": {
    "estimated_duration": 30,
    "energy_cost": "medium"
  }
}
